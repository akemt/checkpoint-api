<?xml version="1.0" encoding="UTF-8"?>
<request>
	<space-name>checkpoint</space-name>
	<quota>100</quota>
	<table-name>VPicInfoTest01</table-name>
	<meta-data>
		<!--外键ID-->
		<element>
			<name>AutoID</name>
			<type>3</type>
			<length>32</length>
			<is-notnull>true</is-notnull>
			<enable-index>true</enable-index>
		</element>
		<!--图片ID-->
		<element>
			<name>PicID</name>
			<type>3</type>
			<length>19</length>
			<is-notnull>true</is-notnull>
			<enable-index>true</enable-index>
		</element>
		<!--经过时间-->
		<element>
			<name>WatchTime</name>
			<type>4</type>
			<enable-index>true</enable-index>
		</element>
		<!--经过时间毫秒-->
		<element>
			<name>WatchTimeMill</name>
			<type>0</type>
			<enable-index>true</enable-index>
			<default-value>-1</default-value>
		</element>
		<!--图像类型-->
		<element>
			<name>ImageType</name>
			<type>0</type>
			<enable-index>true</enable-index>
			<default-value>0</default-value>
		</element>
		<!--图像长-->
		<element>
			<name>ImageWidth</name>
			<type>0</type>
			<enable-index>true</enable-index>
			<default-value>0</default-value>
		</element>
		<!--图像高-->
		<element>
			<name>ImageHeight</name>
			<type>0</type>
			<enable-index>true</enable-index>
			<default-value>0</default-value>
		</element>
	</meta-data>
</request>

<?xml version="1.0" encoding="UTF-8"?>
<request>
	<space-name>checkpoint</space-name>
	<quota>100</quota>
	<table-name>CheckPointInfo</table-name>
	<meta-data>
		<!--主键ID-->
		<element>
			<name>AutoID</name>
			<type>3</type>
			<length>32</length>
			<is-notnull>true</is-notnull>
			<enable-index>true</enable-index>
		</element>
		<!--卡口编号-->
		<element>
			<name>ChPointID</name>
			<type>3</type>
			<length>32</length>
			<enable-index>true</enable-index>
		</element>
		<!--系统编号-->
		<element>
			<name>XTBH</name>
			<type>3</type>
			<length>1</length>
			<enable-index>true</enable-index>
		</element>
		<!--设备编号-->
		<element>
			<name>DeviceID</name>
			<type>3</type>
			<!--<length>32</length>-->
			<length>64</length>
			<is-notnull>true</is-notnull>
			<enable-index>true</enable-index>
		</element>
		<!--设备名称-->
		<element>
			<name>DeviceName</name>
			<type>3</type>
			<length>256</length>
			<enable-index>true</enable-index>
		</element>
		<!--地点名称-->
		<element>
			<name>PlaceName</name>
			<type>3</type>
			<length>256</length>
			<enable-index>true</enable-index>
		</element>
		<!--IP地址-->
		<element>
			<name>IP</name>
			<type>3</type>
			<length>15</length>
			<enable-index>true</enable-index>
		</element>
		<!--端口-->
		<element>
			<name>Port</name>
			<type>0</type>
			<enable-index>true</enable-index>
		</element>
		<!--经度-->
		<element>
			<name>Longitude</name>
			<type>7</type>
			<enable-index>true</enable-index>
		</element>
		<!--纬度-->
		<element>
			<name>Latitude</name>
			<type>7</type>
			<enable-index>true</enable-index>
		</element>
		<!--信息来源 0：洛阳,1：宇视,2：其他 网关提供，不可编辑-->
		<element>
			<name>SourceType</name>
			<type>0</type>			
			<is-notnull>true</is-notnull>
			<enable-index>true</enable-index>
		</element>
		<!--保留字段-->
		<element>
			<name>Reserve1</name>
			<type>0</type>
			<enable-index>true</enable-index>
		</element>
		<!--保留字段-->
		<element>
			<name>Reserve2</name>
			<type>0</type>
			<enable-index>true</enable-index>
		</element>
		<!--保留字段-->
		<element>
			<name>Reserve3</name>
			<type>3</type>
			<!--<length>32</length>-->
			<length>64</length>
			<enable-index>true</enable-index>
		</element>
		<!--CamID-->
		<element>
			<name>CamID</name>
			<type>3</type>
			<length>64</length>
			<enable-index>true</enable-index>
		</element>
		<!--ClusterCode-->
		<element>
			<name>ClusterCode</name>
			<type>3</type>
			<length>32</length>
			<enable-index>true</enable-index>
		</element>
	</meta-data>
</request>

<?xml version="1.0" encoding="UTF-8"?>
<request>
	<space-name>checkpoint</space-name>
	<quota>100</quota>
	<table-name>SubscribeInfoTest02</table-name>
	<meta-data>
		<!--1.订阅标识符-->
		<element>
			<name>SubscribeID</name>
			<type>3</type>
			<length>30</length>
			<is-notnull>true</is-notnull>
			<is-key>true</is-key>
		</element>
		<!--2.订阅标题-->
		<element>
			<name>Title</name>
			<type>3</type>
		</element>
		<!--3.类别-->
		<element>
			<name>SubCategory</name>
			<type>3</type>
			<is-notnull>true</is-notnull>
			<enable-index>true</enable-index>
			<default-value>3</default-value>
		</element>
		<!--4.内容类别-->
		<element>
			<name>SubDetail</name>
			<type>3</type>
			<enable-index>true</enable-index>
			<default-value>8</default-value>
		</element>
		<!--5.订阅资源路径-->
		<element>
			<name>ResourceURI</name>
			<type>3</type>
			<length>260</length>
			<is-notnull>true</is-notnull>
			<enable-index>true</enable-index>
		</element>
		<!--6.申请人-->
		<element>
			<name>ApplicantName</name>
			<type>3</type>
			<length>20</length>
			<enable-index>true</enable-index>
		</element>
		<!--7.申请单位-->
		<element>
			<name>ApplicantOrg</name>
			<type>3</type>
			<length>50</length>
		</element>
		<!--8.开始时间-->
		<element>
			<name>StartTime</name>
			<type>4</type>
		</element>
		<!--9.结束时间-->
		<element>
			<name>EndTime</name>
			<type>4</type>
		</element>
		<!--10.信息接收地址-->
		<element>
			<name>ReceiveAddr</name>
			<type>3</type>
			<length>260</length>
		</element>
		<!--11.理由-->
		<element>
			<name>Reason</name>
			<type>3</type>
			<length>200</length>
		</element>
		<!--12.订阅取消单位 -->
		<element>
			<name>SubCancelOrg</name>
			<type>3</type>
			<length>50</length>
		</element>
		
		<!--13.订阅取消人-->
		<element>
			<name>SubCancelPerson</name>
			<type>3</type>
			<length>20</length>
		</element>
		<!--14.取消时间-->
		<element>
			<name>CancelTime</name>
			<type>4</type>
			<enable-index>true</enable-index>
		</element>
		<!--15.取消原因-->
		<element>
			<name>CancelReason</name>
			<type>3</type>
			<length>50</length>
		</element>
		<!--16.status-->
		<element>
			<name>status</name>
			<type>0</type>
		</element>
	</meta-data>
</request>
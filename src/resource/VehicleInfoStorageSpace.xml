<?xml version="1.0" encoding="UTF-8"?>
<request>
	<space-name>checkpoint</space-name>
	<quota>100</quota>
	<table-name>VehicleInfo</table-name>
	<meta-data>
		<!--主键ID-->
		<element>
			<name>AutoID</name>
			<type>3</type>
			<length>32</length>
			<is-notnull>true</is-notnull>
			<is-key>true</is-key>
		</element>
		<!--应用类型-->
		<element>
			<name>AppType</name>
			<type>0</type>
			<default-value>0</default-value>
		</element>
		<!--信息来源-->
		<element>
			<name>SourceType</name>
			<type>0</type>
			<is-notnull>true</is-notnull>
			<default-value>3</default-value>
		</element>
		<!--系统编号-->
		<element>
			<name>XTBH</name>
			<type>3</type>
			<length>2</length>
			<enable-index>true</enable-index>
		</element>
		<!--设备编号-->
		<element>
			<name>DeviceID</name>
			<type>3</type>
			<!--<length>32</length>-->
			<length>64</length>
			<enable-index>true</enable-index>
			<is-notnull>true</is-notnull>
		</element>
		<!--经过时间-->
		<element>
			<name>WatchTime</name>
			<type>4</type>
			<enable-index>true</enable-index>
		</element>
		<!--经过时间毫秒-->
		<element>
			<name>WatchTimeMill</name>
			<type>0</type>
			<default-value>-1</default-value>
		</element>
		<!--上传时间-->
		<element>
			<name>UploadTime</name>
			<type>4</type>
			<is-notnull>true</is-notnull>
			<enable-index>true</enable-index>
		</element>
		<!--设备监测地点-->
		<element>
			<name>DeviceDesc</name>
			<type>3</type>
			<length>256</length>
			<enable-index>true</enable-index>
		</element>
		<!--域编码-->
		<element>
			<name>DomCode</name>
			<type>3</type>
			<length>32</length>
		</element>
		<!--NVRCode-->
		<element>
			<name>NVRCode</name>
			<type>3</type>
			<length>32</length>
		</element>
		<!--车辆外形 -->
		<element>
			<name>VehicleBody</name>
			<type>3</type>
			<length>2</length>
		</element>
		
		<!--车辆类型-->
		<element>
			<name>CarType</name>
			<type>3</type>
			<length>2</length>
			<enable-index>true</enable-index>
		</element>
		<!--车外廓长-->
		<element>
			<name>VehicleLength</name>
			<type>0</type>
			<default-value>0</default-value>
		</element>
		<!--车身颜色-->
		<element>
			<name>VehicleColor</name>
			<type>3</type>
			<length>2</length>
			<enable-index>true</enable-index>
		</element>
		<!--车身颜色深浅-->
		<element>
			<name>VehicleColorDept</name>
			<type>3</type>
			<length>2</length>
		</element>
		<!--车辆速度-->
		<element>
			<name>CarSpeed</name>
			<type>0</type>
			<enable-index>true</enable-index>
			<default-value>0</default-value>
		</element>
		<!--限速-->
		<element>
			<name>LimitedSpeed</name>
			<type>0</type>
			<default-value>0</default-value>
		</element>
		
		<!--车牌号-->
		<element>
			<name>CarNo</name>
			<type>3</type>
			<length>32</length>
			<is-notnull>true</is-notnull>
			<enable-index>true</enable-index>
		</element>
		<!-- 识别置信度-->
		<element>
			<name>Confidence</name>
			<type>7</type>
			<default-value>0.00</default-value>
		</element>
		<!-- 号牌种类-->
		<element>
			<name>PlateType</name>
			<type>3</type>
			<enable-index>true</enable-index>
		</element>
		<!--号牌颜色-->
		<element>
			<name>PlateColor</name>
			<type>3</type>
			<length>2</length>
			<is-notnull>true</is-notnull>
			<enable-index>true</enable-index>
		</element>
		<!--车辆品牌-->
		<element>
			<name>CarLogo</name>
			<type>3</type>
			<length>2</length>
			<enable-index>true</enable-index>
		</element>
		<!--行驶状态-->
		<element>
			<name>DriveStatus</name>
			<type>3</type>
			<length>4</length>
		</element>
		<!--处理标记-->
		<element>
			<name>DealTag</name>
			<type>3</type>
			<length>2</length>
		</element>
		<!--车道-->
		<element>
			<name>CarRoad</name>
			<type>3</type>
			<length>2</length>
		</element> 
		<!--方向编号-->
		<element>
			<name>Direction</name>
			<type>3</type>
			<length>2</length>
			<enable-index>true</enable-index>
		</element>
		<!--图片数-->
		<element>
			<name>PicNum</name>
			<type>0</type>
			<default-value>0</default-value>
		</element>
	</meta-data>
</request>